{
  "name": "erpnext-cdc-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "host.docker.internal",
    "database.port": "3306",
    "database.user": "root",
    "database.password": "admin",
    "database.server.id": "184664",
    "topic.prefix": "erpnext",
    "database.include.list": "_1f2b3e1ef71e8d5b",
    "table.include.list": "_1f2b3e1ef71e8d5b.tabEmployee,_1f2b3e1ef71e8d5b.tabAttendance",
    "schema.history.internal.kafka.bootstrap.servers": "kafka:9092",
    "schema.history.internal.kafka.topic": "schema-changes.erpnext",
    "schema.history.internal.consumer.security.protocol": "PLAINTEXT",
    "schema.history.internal.producer.security.protocol": "PLAINTEXT",
    "include.schema.changes": "true",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "snapshot.mode": "initial",
    "snapshot.locking.mode": "none",
    "database.allowPublicKeyRetrieval": "true",
    "decimal.handling.mode": "string",
    "bigint.unsigned.handling.mode": "long",
    "time.precision.mode": "adaptive_time_microseconds"
  }
}